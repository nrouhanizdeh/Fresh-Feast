      <div id="start-screen" class="table-wrapper justify-content-center text-center  is-fluid transbox fade-in-start">
        <button type="button" class="btn btn-info add-new"><i class="fa fa-plus"></i> Add New</button>
        <table class="table table-bordered">
          <tbody>
            <tr>
              <th>Food Name</th>
              <th>Days Fresh</th>
              <th>Expiration</th>
              <th></th>
            </tr>
            <tr>
              {{#each fridge}}
              {{> components/fridgerow-template}}
              {{/each}}
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>





  </div>
</main>